<div class="col-<%= default_grid_system %>-9">
  <div class="panel panel-default">
    <div class="panel-body">
      <div class="lead"><%= t ".title" %></div>
      <table class="table table-hover">
        <% @users.each do |user| %>
          <tr>
            <td><%= link_to user.full_name, user_path(user) %></td>
            <td><%= user.email %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>